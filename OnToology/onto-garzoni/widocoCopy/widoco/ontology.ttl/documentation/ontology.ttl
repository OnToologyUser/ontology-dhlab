      a       owl:Class ;
      rdfs:comment "The surname or family name of a person entity."@en ;
      rdfs:label "Surname"@en , "Nom de famille"@fr ;
      rdfs:seeAlso "http://xmlns.com/foaf/0.1/familyName"^^xsd:anyURI ;
